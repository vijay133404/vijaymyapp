  <%=form_for :blog, url:blogs_path do |f|%>
<p>
<%=f.label :title%>
<%=f.text_field :title%>
</p>
<p>
<%=f.label :description%>
<%=f.text_field :description%>
</p>

<p>
<%=f.label :image%>
<%=f.file_field :image%>
</p>
<p>
<%= f.select(:categry_id, Category.all.collect {|u| [u.name, u.id]}, :prompt => 'Select Category') %> 
</p>
<p>

<%=f.submit %>
</p>
<%end%>
<%= link_to 'Back', blogs_path %>
